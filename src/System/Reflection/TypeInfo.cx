namespace System.Reflection;

public struct TypeInfo {
    public const ulong Hash;
    public const uint Flags;
    public const uint Size;
    public const string Name;
    public const string Namespace;
    public const TypeInfo[] GenericParams;
    public const TypeInfo? BaseType;
    public const TypeInfo[] Interfaces;
    public const FieldInfo[] Fields;
    public const FunctionInfo[] Functions;
}
